<%
  def link_to(name, url)
    return "<a href= #{url}>#{name}</a>"
  end
%>

<html>
  <body>
    <h1>You're using a layout</h1>
    <p>
      <h3> I'm a header</h3>
      <p><%= link_to('Google','www.google.com') %></p>
      <a href='me.html'>Me</a>

    </p>
    <hr />
    <div>
      <%= yield %>
    </div>
    <hr />
    <div>
      <h3> I'm a footer</h3>
      <p><%= link_to('Google','www.google.com')%></p>
    </div>
  </body>
</html>